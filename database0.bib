@article{DRELL1971578,
title = "Partons and their applications at high energies",
journal = "Annals of Physics",
volume = "66",
number = "2",
pages = "578 - 623",
year = "1971",
issn = "0003-4916",
doi = "https://doi.org/10.1016/0003-4916(71)90071-6",
url = "http://www.sciencedirect.com/science/article/pii/0003491671900716",
author = "Sidney D Drell and Tung-Mow Yan",
abstract = "We discuss Feynman's parton model for deep inelastic weak or electromagnetic processes as an application of the impulse approximation to elementary particle interactions. The special features and conditions permitting this application are elaborated upon in some detail including the dependence of the parton model and the impulse treatment on an appropriate choice of coordinate frames and the role of the very soft or “wee” partons. Application of the parton model is made to the calculation of the cross section for massive lepton pair production in very high energy hadron-hadron collisions and compared with experiment. The conjectured role of light cone singularities in describing this and the other deep inelastic amplitudes is also discussed."
}

